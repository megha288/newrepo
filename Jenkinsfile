pipeline {
    agent any 
    stages {
        stage('Stage develop') {
            steps {
                echo 'develop world!' 
            }
        }
        stage('Stage build') {
            steps {
                echo 'build world!' 
            }
        }
        stage('Stage test') {
            steps {
                echo 'test world!' 
            }
        }
        stage('Stage deploy') {
            steps {
                echo 'deploy world!' 
            }
        }
        
    }
}
